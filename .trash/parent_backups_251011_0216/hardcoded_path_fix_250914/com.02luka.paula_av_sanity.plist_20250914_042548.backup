    </array>
    <key>KeepAlive</key><false/>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key><string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>
    <key>WorkingDirectory</key><string>/Users/icmini/02luka</string>
  </dict>
</plist>