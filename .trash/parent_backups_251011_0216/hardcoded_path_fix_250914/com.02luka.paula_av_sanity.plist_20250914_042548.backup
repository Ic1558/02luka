<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key><string>com.02luka.paula_av_sanity</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/python3</string>
      <string>/Users/icmini/02luka/monitors/paula_av_sanity.py</string>
    </array>
    <key>StandardOutPath</key><string>/Users/icmini/02luka/logs/paula_av_sanity.out</string>
