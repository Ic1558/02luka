<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key><string>com.02luka.mary.alerts</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/zsh</string>
    <string>-lc</string>
    <string>$HOME/02luka/tools/mary_alerts_watch.zsh</string>
  </array>
  <key>WorkingDirectory</key><string>$HOME/02luka</string>
  <key>StartInterval</key><integer>60</integer>
  <key>RunAtLoad</key><true/>
  <key>StandardOutPath</key><string>$HOME/02luka/logs/mary_alerts.out.log</string>
  <key>StandardErrorPath</key><string>$HOME/02luka/logs/mary_alerts.err.log</string>
  <key>EnvironmentVariables</key>
  <dict>
    <key>LUKA_SOT</key><string>$HOME/02luka</string>
    <key>LUKA_HOME</key><string>$HOME/02luka/g</string>
  </dict>
</dict>
</plist>
