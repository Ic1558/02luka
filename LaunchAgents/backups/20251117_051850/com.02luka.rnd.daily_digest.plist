<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0"><dict>
  <key>Label</key><string>com.02luka.rnd.daily_digest</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/zsh</string>
    <string>-lc</string>
    <string>~/02luka/tools/rnd_daily_digest.zsh</string>
  </array>
  <key>StartCalendarInterval</key>
  <dict><key>Hour</key><integer>7</integer><key>Minute</key><integer>5</integer></dict>
  <key>RunAtLoad</key><false/>
  <key>StandardOutPath</key><string>~/02luka/logs/rnd_daily_digest.out.log</string>
  <key>StandardErrorPath</key><string>~/02luka/logs/rnd_daily_digest.err.log</string>
</dict></plist>
