<plist version="1.0">
<dict>
  <key>Label</key><string>com.02luka.cls.rotate</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/zsh</string>
    <string>-lc</string>
    <string>$HOME/02luka/g/tools/cls/cls_rotate_logs.zsh</string>
