<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key><string>com.02luka.logs.rotate.daily</string>
    <key>ProgramArguments</key>
    <array>
      <string>/bin/zsh</string>
      <string>-lc</string>
      <string>$HOME/02luka/g/tools/log_rotate.zsh</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key><integer>4</integer>
      <key>Minute</key><integer>40</integer>
    </dict>
    <key>StandardOutPath</key><string>$HOME/02luka/logs/log_rotate.out.log</string>
    <key>StandardErrorPath</key><string>$HOME/02luka/logs/log_rotate.err.log</string>
    <key>RunAtLoad</key><true/>
  </dict>
</plist>
